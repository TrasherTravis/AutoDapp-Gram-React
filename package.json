{
  "name": "autodapp-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "grapesjs": "^0.17.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "laravel-mix": "^6.0.25",
    "notyf": "^3.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "mix",
    "prod": "mix --production",
    "watch": "mix watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "productName": "autodapp",
    "appId": "autodapp.app",
    "directories": {
      "output": "export",
      "buildResources": "build"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "nsis": {
    "installerIcon": "public/favicon.ico",
    "installerHeaderIcon": "public/favicon.ico",
    "deleteAppDataOnUninstall": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "sass": "^1.53.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.1.0"
  }
}
